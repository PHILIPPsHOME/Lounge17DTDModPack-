<configs>
	<append xpath="/ui_display_info/item_display">
		<!-- Food/Drink -->
		<item_display_info display_type="NukaDisplay1" display_group="groupConsumables">
			<display_entry name="dMaxHealthBonus" title_key="statMaxHealthBonus" display_leading_plus="true"/>
			<display_entry name="foodHealthAmount" title_key="statHealthAmount"/>
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>
			<display_entry name="dStaminaRegen" title_key="statStaminaRegen" display_type="Percent"/>
			<display_entry name="dRunSpeed" title_key="statRunSpeed" display_type="Percent" display_leading_plus="true"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>
		<item_display_info display_type="NukaDisplay2" display_group="groupConsumables">
			<display_entry name="dEntityDamageBrawling" title_key="statEntityDamageBrawling" display_type="Percent" display_leading_plus="true"/>
			<display_entry name="dMaxHealthBonus" title_key="statMaxHealthBonus" display_leading_plus="true"/>
			<display_entry name="foodHealthAmount" title_key="statHealthAmount"/>
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>
			<display_entry name="dStaminaRegen" title_key="statStaminaRegen" display_type="Percent"/>
			<display_entry name="dRunSpeed" title_key="statRunSpeed" display_type="Percent" display_leading_plus="true"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>
		<item_display_info display_type="SunsetDisplay" display_group="groupConsumables">
			<display_entry name="dMaxHealthBonus" title_key="statMaxHealthBonus" display_leading_plus="true"/>
			<display_entry name="foodHealthAmount" title_key="statHealthAmount"/>
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>
			<display_entry name="dStaminaRegen" title_key="statStaminaRegen" display_type="Percent"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>
		<item_display_info display_type="goodFoodDisplay" display_group="groupConsumables">
			<display_entry name="$foodAmountAdd" title_key="statFoodAmount"/>
			<display_entry name="foodHealthAmount" title_key="statHealthAmount"/>
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>
			<display_entry name=".foodStaminaBonusAdd" title_key="statFoodStaminaBonus"/>
			<display_entry name="dEfficientDigestion" title_key="statEfficientDigestion" display_type="Percent" display_leading_plus="true"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>
		<!-- Drugs -->
		<item_display_info display_type="cureRadiation" display_group="groupConsumables">
			<display_entry name="dCureRadiation" title_key="statCureRadiation" display_type="Percent"/>
		</item_display_info>
		<item_display_info display_type="radiationResist" display_group="groupConsumables">
			<display_entry name="dRadiationResist" title_key="statRadiationResist" display_type="Percent"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>
		<item_display_info display_type="addictionCure" display_group="groupConsumables">
			<display_entry name="dAddictionCure" title_key="statAddictionCure" display_type="Percent"/>
		</item_display_info>
		<item_display_info display_type="venomResist" display_group="groupConsumables">
			<display_entry name="dVenomResist" title_key="statVenomResist" display_type="Percent"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>
		<item_display_info display_type="drugBeZerk" display_group="groupConsumables" >
			<display_entry name="dEntityDamageAll" title_key="statEntityDamageAll" display_type="Percent" display_leading_plus="true"/>
			<display_entry name="dHealthLoss" title_key="statHealthLoss" display_type="Percent" display_leading_plus="true"/>
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>
		<item_display_info display_type="drugBraintats" display_group="groupConsumables" >
			<display_entry name="dAttributeInt" tags="attIntellect" title_key="statShowIntellect" display_leading_plus="true"/>
			<display_entry name="dAttributePer" tags="attPerception" title_key="statShowPerception" display_leading_plus="true"/>
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>
		<item_display_info display_type="drugBuffin" display_group="groupConsumables" >
			<display_entry name="dAttributeStr" tags="attStrength" title_key="statShowStrength" display_leading_plus="true"/>
			<display_entry name="dAttributeFor" tags="attFortitude" title_key="statShowFortitude" display_leading_plus="true"/>
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>
		<item_display_info display_type="drugCalmZ" display_group="groupConsumables" >
			<display_entry name="dAttributeAgl" tags="attAgility" title_key="statShowAgility" display_leading_plus="true"/>
			<display_entry name="dAttributePer" tags="attPerception" title_key="statShowPerception" display_leading_plus="true"/>
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>
		<item_display_info display_type="drugHGL" display_group="groupConsumables" >
			<display_entry name="dAttributeLck" tags="attLuck" title_key="statShowLuck" display_leading_plus="true"/>
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>
		<item_display_info display_type="drugMedZ" display_group="groupConsumables" >
			<display_entry name="dHealthLoss" title_key="statHealthLoss" display_type="Percent" display_leading_plus="true"/>
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>
		<!-- Weapons -->
		<item_display_info display_type="EnergyDisplay" display_group="groupRanged">
			<display_entry name="EntityDamage" title_key="statEntityDamageRanged" tags="perkNewAgeCowboy,admin"/>
			<display_entry name="MagazineSize" title_key="statMagazineSize" tags="perkNewAgeCowboy,admin"/>
			<display_entry name="RoundsPerMinute" title_key="statRoundsPerMinute" tags="perkNewAgeCowboy,admin"/>						
			<display_entry name="DamageFalloffRange" title_key="statDamageFalloffRange" tags="perkNewAgeCowboy,admin"/>
			<display_entry name="DegradationMax" title_key="statDegradationMax" tags="perkNewAgeCowboy,admin"/>
		</item_display_info>
		<item_display_info display_type="rangedGunHeavy" display_group="groupRanged">
			<display_entry name="EntityDamage" display_type="Decimal1" title_key="statEntityDamageRanged" tags="perkHeavyGunner,admin"/>
			<display_entry name="MagazineSize" title_key="statMagazineSize" tags="perkHeavyGunner,admin"/>
			<display_entry name="RoundsPerMinute" title_key="statRoundsPerMinute" tags="perkHeavyGunner,admin"/>
			<display_entry name="DamageFalloffRange" title_key="statDamageFalloffRange" tags="perkHeavyGunner,admin"/>
			<display_entry name="DegradationMax" title_key="statDegradationMax" tags="perkHeavyGunner,admin"/>
		</item_display_info>
		<!-- Armor Mods -->
		<item_display_info display_type="modPARadar" display_group="modAttire">
			<display_entry name="dRadarRange" title_key="statRadarRange" display_leading_plus="true"/>
		</item_display_info>
		<item_display_info display_type="modPABlock" display_group="modAttire">
			<display_entry name="dBlockDamage" title_key="statBlockDamage" display_leading_plus="true"/>
		</item_display_info>
		<!-- Attribute Statues -->
		<item_display_info display_type="paStatueStrengthI" display_group="groupBlocks">
			<display_entry name="dAttributeStr" tags="attStrength" title_key="statShowStrength" display_leading_plus="true"/>
		</item_display_info>
		<item_display_info display_type="paStatuePerceptionI" display_group="groupBlocks">
			<display_entry name="dAttributePer" tags="attPerception" title_key="statShowPerception" display_leading_plus="true"/>
		</item_display_info>
		<item_display_info display_type="paStatueIntellectI" display_group="groupBlocks">
			<display_entry name="dAttributeInt" tags="attIntellect" title_key="statShowIntellect" display_leading_plus="true"/>
		</item_display_info>
		<item_display_info display_type="paStatueFortitudeI" display_group="groupBlocks">
			<display_entry name="dAttributeFor" tags="attFortitude" title_key="statShowFortitude" display_leading_plus="true"/>
		</item_display_info>
		<item_display_info display_type="paStatueSurvivalI" display_group="groupBlocks">
			<display_entry name="dAttributeSur" tags="attSurvival" title_key="statShowSurvival" display_leading_plus="true"/>
		</item_display_info>
		<item_display_info display_type="paStatueAgilityI" display_group="groupBlocks">
			<display_entry name="dAttributeAgl" tags="attAgility" title_key="statShowAgility" display_leading_plus="true"/>
		</item_display_info>
		<item_display_info display_type="paStatueLuckI" display_group="groupBlocks">
			<display_entry name="dAttributeLck" tags="attLuck" title_key="statShowLuck" display_leading_plus="true"/>
		</item_display_info>
	</append>
</configs>